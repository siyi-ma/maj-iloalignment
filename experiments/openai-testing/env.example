# Environment Configuration Template

# Copy this file to .env and fill in your actual API key
# DO NOT commit the .env file to version control

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Organization ID (if using organization account)
# OPENAI_ORG_ID=your_org_id_here

# API Configuration
# OPENAI_BASE_URL=https://api.openai.com/v1  # Custom base URL if needed
# OPENAI_TIMEOUT=30000  # Request timeout in milliseconds

# Experiment Configuration
# MAX_RETRIES=3
# RATE_LIMIT_DELAY=1000  # Delay between requests in ms

# Development Settings
NODE_ENV=development
DEBUG=true
